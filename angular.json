{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ontimize-web-ngx-nvd3": {
      "projectType": "library",
      "root": "projects/ontimize-web-ngx-nvd3",
      "sourceRoot": "projects/ontimize-web-ngx-nvd3/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ontimize-web-ngx-nvd3/tsconfig.lib.json",
            "project": "projects/ontimize-web-ngx-nvd3/ng-package.json"
          },
          "scripts": [
            "projects/ontimize-web-ngx-nvd3/src/lib/custom_models/gauge.js",
            "projects/ontimize-web-ngx-nvd3/src/lib/custom_models/gaugeChart.js",
            "projects/ontimize-web-ngx-nvd3/src/lib/custom_models/packedBubble.js",
            "projects/ontimize-web-ngx-nvd3/src/lib/custom_models/packedBubbleChart.js",
            "projects/ontimize-web-ngx-nvd3/src/lib/custom_models/radar.js",
            "projects/ontimize-web-ngx-nvd3/src/lib/custom_models/radarChart.js"
          ]
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ontimize-web-ngx-nvd3/src/test.ts",
            "tsConfig": "projects/ontimize-web-ngx-nvd3/tsconfig.spec.json",
            "karmaConfig": "projects/ontimize-web-ngx-nvd3/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ontimize-web-ngx-nvd3/tsconfig.lib.json",
              "projects/ontimize-web-ngx-nvd3/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "ontimize-web-ngx-nvd3"
}